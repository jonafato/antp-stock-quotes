{
    "name": "Stock Quotes [ANTP]",
    "version": "0.1",
    "manifest_version": 2,
    "description": "A simple stock quote widget for Awesome New Tab Page",
    "icons": {
    },
    "background": {
        "scripts": [
            "js/background.js"
        ],
        "persistent": false
    },
    "web_accessible_resources": [
        "widget.html"
    ],
    "content_security_policy": "script-src 'self'; object-src 'self'",
    "permissions": [
        "storage",
        "http://query.yahooapis.com/v1/public/yql*"
    ]
}
